{
  "name": "interactive-beam-side-panel",
  "version": "0.1.0",
  "description": "A side panel providing information of current interactive environment, data sets associated with PCollections, pipeline state, job status and source capture.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/KevinGG/beam/tree/master/sdks/python/apache_beam/runners/interactive",
  "bugs": {
    "url": "https://github.com/kevingg/beam/issues"
  },
  "license": "BSD-3-Clause",
  "author": "ningk",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevingg/beam/sdks/python/apache_beam/runners/interactive.git#side"
  },
  "scripts": {
    "build": "jlpm run build:lib",
    "build:labextension": "cd interactive_beam_side_panel && rimraf labextension && mkdirp labextension && cd labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:all": "jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf interactive_beam_side_panel/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.1.1",
    "@jupyterlab/apputils": "^2.1.0",
    "@jupyterlab/coreutils": "^4.0.0",
    "@jupyterlab/launcher": "^2.1.1",
    "@jupyterlab/mainmenu": "^2.1.1",
    "@jupyterlab/outputarea": "^2.1.1",
    "@jupyterlab/rendermime": "^2.1.1",
    "@jupyterlab/services": "^5.0.0",
    "@lumino/messaging": "^1.3.3",
    "@lumino/widgets": "^1.12.0",
    "@material/select": "^6.0.0",
    "@rmwc/button": "^6.0.14",
    "@rmwc/card": "^6.0.14",
    "@rmwc/checkbox": "^6.0.14",
    "@rmwc/drawer": "^6.0.14",
    "@rmwc/fab": "^6.0.14",
    "@rmwc/grid": "^6.0.14",
    "@rmwc/icon": "^6.0.14",
    "@rmwc/list": "^6.0.14",
    "@rmwc/top-app-bar": "^6.0.14",
    "@types/d3": "^5.7.2",
    "@types/d3-force": "^1.2.1",
    "@types/d3-zoom": "^1.7.4",
    "d3": "^5.16.0",
    "d3-zoom": "^1.8.3",
    "material-design-icons": "^3.0.1",
    "typescript-formatter": "^7.2.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "mkdirp": "^1.0.3",
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "interactive_beam_side_panel"
        }
      }
    },
    "extension": true
  },
  "test": "jest",
  "resolutions": {
    "@types/react": "~16.9.16"
  }
}
